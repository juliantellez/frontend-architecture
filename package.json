{
  "name": "frontend-architecture",
  "version": "0.0.1",
  "description": "proposed frontend architecture",
  "main": "index.js",
  "scripts": {
    "types:sass": "rm -rf **/*.scss.d.ts && tsm src --camelCase",
    "types:sass:watch": "npm run types:sass -- --watch",
    "start": "TS_NODE_PROJECT=tsconfig.json webpack-dev-server --env.NODE_ENV=development --config ./config/webpack/webpack.config.ts",
    "dev": "npm run types:sass:watch | npm run start",
    "build": "rm -rf dist && npm run types:sass && TS_NODE_PROJECT=tsconfig.json webpack --env.NODE_ENV=production --config ./config/webpack/webpack.config.ts",
    "build:analyze": "rm -rf dist && npm run types:sass && TS_NODE_PROJECT=tsconfig.json webpack ---env.analyze --env.NODE_ENV=production --config ./config/webpack/webpack.config.ts",
    "lint": "eslint --ext .tsx --ext .ts ./src",
    "lint:fix": "npm run lint -- --fix",
    "test": "TS_NODE_PROJECT=tsconfig.json NODE_ENV=test mocha --opts ./config/mocha/mocha.opts",
    "test:watch": "npm run test -- --watch-extensions ts --watch",
    "test:coverage": "nyc npm run test"
  },
  "nyc": {
    "extends": "./config/nyc/.nycrc.json"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/chai": "^4.2.7",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.7",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/uuid": "^3.4.6",
    "@types/webpack": "^4.41.2",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "@types/webpack-dev-server": "^3.9.0",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^16.0.0",
    "jsdom-global": "^3.0.2",
    "mocha": "6.1.4",
    "mock-css-modules": "^1.0.0",
    "mock-local-storage": "^1.1.11",
    "node-sass": "^4.13.1",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "react-router-dom": "^5.1.2",
    "rxjs-marbles": "^5.0.4",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.2",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typed-scss-modules": "^1.0.1",
    "typescript": "^3.7.5",
    "uuid": "^3.4.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
