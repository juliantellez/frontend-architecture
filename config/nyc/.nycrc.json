{
    "all": true,
    "check-coverage": true,
    "extension": [
        ".ts",
        ".tsx"
    ],
    "include": [
        "src"
    ],
    "exclude": [
        "coverage",
        "config",
        "dist"
    ],
    "reporter": [
        "lcov",
        "text",
        "text-summary"
    ],
    "skip-full": true,
    "statements": 98,
    "branches": 98,
    "functions": 98,
    "lines": 98
}
